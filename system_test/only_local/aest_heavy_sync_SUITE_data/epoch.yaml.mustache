{{#aeternity_config}}
---
peers: {{^peers}}[]{{/peers}}
{{#peers}}
    - {{peer}}
{{/peers}}

sync:
    port: {{services.sync.port}}
    ping_interval: 5000
    single_outbound_per_group: false

http:
    external:
        port: {{services.ext_http.port}}
    internal:
        listen_address: 0.0.0.0
        port: {{services.int_http.port}}

keys:
    peer_password: {{key_password}}
    dir: ./keys

chain:
    persist: {{config.persist}}

mining:
    beneficiary: "ak_2VoAhMd7tVJrDYM5vPJwFRjueZyirDJumVJNeBWL9j1eNTHsRx"
    micro_block_cycle: 3000
    expected_mine_rate: 180000
    autostart: {{mining.autostart}}
    cuckoo:
        edge_bits: 29
        miners:
            - executable: mean29-generic
              extra_args: "-t 5"

fork_management:
    network_id: "ae_system_test"
{{/aeternity_config}}
